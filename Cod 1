Servo servo7;
Servo servo8;
Servo servo9;//-
Servo servo10;
Servo servo11;
Servo servo12;//--

void  pozitie(int R,int T,int X){
    servo4.write(R);
    servo5.write(T);
    servo6.write(X); 
}
void sky(int A,int B,int C){
    servo1.write(A);
    servo2.write(B);
    servo3.write(C); 
}
void srt(int D,int E,int F){
    servo7.write(D);
    servo8.write(E);
    servo9.write(F); 
}
void nsa(int G,int H,int I){
    servo10.write(G);
    servo11.write(H);
    servo12.write(I); 
}
void ugh(int J,int K,int L,int M){
    
}
void setup() 
  {
    servo2.attach(2);
    servo1.attach(3);
    servo3.attach(4);

    servo5.attach(50);
    servo4.attach(52);
    servo6.attach(48);

    servo7.attach(46);
    servo8.attach(44);
    servo9.attach(42);
  }
 //mEEEEEEEEEEEEEEEEEEEEEEEEEN
  void twk(int W,int X,int Y,int Z){
   servo2.write(W);
   servo3.write(X);
   servo5.write(Y);
   servo6.write(Z);
  }
