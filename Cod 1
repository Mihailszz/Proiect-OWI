void loop(){
sky(20,150,70);
    delay(300);
    sky(20,90,90);
    delay(300);
pozitie(150,50,110);
    delay(300);
    pozitie(150,90,90);
    delay(300);
srt(20,150,70);
    delay(300);
    srt(20,90,90);
    delay(300);    
nsa(150,50,110);
    delay(300);
    nsa(150,90,90);
    delay(300);
   sky(130,90,90);
    delay(300);
     pozitie(50,90,90);
    delay(300);   
    srt(130,90,90);
    delay(300); 
    nsa(50,90,90);
    delay(300); 
}
  
